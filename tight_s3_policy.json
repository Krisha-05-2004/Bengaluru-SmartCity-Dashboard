{
  "Version":"2012-10-17",
  "Statement":[
    {
      "Effect":"Allow",
      "Action":["s3:ListBucket"],
      "Resource":["arn:aws:s3:::smartcity-processed-075540751057"],
      "Condition": {
        "StringLike": {
          "s3:prefix": ["processed/Bengaluru/*", "raw/Bengaluru/*"]
        }
      }
    },
    {
      "Effect":"Allow",
      "Action":["s3:GetObject", "s3:PutObject"],
      "Resource":[
        "arn:aws:s3:::smartcity-processed-075540751057/processed/Bengaluru/*",
        "arn:aws:s3:::smartcity-raw-075540751057/raw/Bengaluru/*"
      ]
    }
  ]
}
